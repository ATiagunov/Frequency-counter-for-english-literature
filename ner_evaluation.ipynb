    "tokens = []\n",
    "for sent in token_sent:\n",
    "  for i in word_tokenize(sent):\n",
    "    tokens.append(i)\n",
    "tags = nltk.pos_tag(tokens)\n",
    "dict_nltk = {}\n",
    "for word, tag in tags:\n",
    "  if tag == 'NNP':\n",
    "    if word not in dict_nltk:\n",
    "      dict_nltk[word] = 1\n",
    "    else:\n",
    "      dict_nltk[word] += 1\n",
    "\n",
    "DeepDiff(dict_true, dict_nltk)"
